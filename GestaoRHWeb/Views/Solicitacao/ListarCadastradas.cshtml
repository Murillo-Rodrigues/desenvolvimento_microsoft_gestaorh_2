@model List<Solicitacao>

@{ Layout = "~/Views/Shared/_Layout.cshtml"; }

<br />
<br />
<br />
<fieldset>
    <legend>Listagem de Solicitações</legend>

    <table class="table table-hover table-striped table-bordered" style="text-align:center">
        <thead>
            <tr class="table-info">
                <th scope="row">#</th>
                <th scope="col">Usuario</th>
                <th scope="col">CarrinhoId</th>
                <th scope="col">Criado em</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Solicitacao solicitacoesCadastradas in Model)
            {
<tr>
    <td>@solicitacoesCadastradas.Id</td>
    <td>@solicitacoesCadastradas.Usuario</td>
    <td>@solicitacoesCadastradas.CarrinhoId</td>
    <td>@solicitacoesCadastradas.CriadoEm</td>
</tr>
}
        </tbody>
    </table>

</fieldset>
